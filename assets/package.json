{
    "type": "module",
    "dependencies": {
        "@egoist/tailwindcss-icons": "^1.9.0",
        "@fontsource/geist-sans": "^5.2.5",
        "@iconify-json/lucide": "^1.2.41",
        "@inertiajs/svelte": "^2.0.9",
        "@tailwindcss/typography": "^0.5.16",
        "axios": "^1.9.0",
        "daisyui": "^5.0.35"
    },
    "devDependencies": {
        "@tsconfig/svelte": "^5.0.4",
        "esbuild": "^0.25.4",
        "esbuild-svelte": "^0.9.2",
        "prettier": "^3.5.3",
        "prettier-plugin-svelte": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "svelte-preprocess": "^6.0.3",
        "typescript": "^5.8.3"
    },
    "prettier": {
        "useTabs": false,
        "tabWidth": 4,
        "singleQuote": false,
        "trailingComma": "all",
        "printWidth": 120,
        "plugins": [
            "prettier-plugin-svelte",
            "prettier-plugin-tailwindcss"
        ],
        "overrides": [
            {
                "files": "*.svelte",
                "options": {
                    "parser": "svelte"
                }
            }
        ]
    }
